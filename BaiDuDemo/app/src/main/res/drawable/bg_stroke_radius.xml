<?xml version="1.0" encoding="utf-8"?>
<!--首页停车详情-->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape>
            <stroke
                android:dashGap="10dp"
                android:dashWidth="10dp"
                android:width="1dp"
                android:color="#aeaeae" />
            <corners android:radius="5dp" />
        </shape>
    </item>
    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp">
        <shape>
            <stroke
                android:width="1dp"
                android:color="@null" />

            <solid android:color="#37bdbe" />

            <corners android:radius="5dp" />
        </shape>
    </item>

</layer-list>