<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#f2f2f2">
    <View
        android:id="@+id/view_no_renzheng"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:visibility="gone"/>
    <LinearLayout
        android:id="@+id/ll_no_renzheng"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="#fff"
        android:visibility="gone">
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#e5e5e5"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="57dp"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/tv_listCarNum"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#333"
                android:layout_gravity="center"
                android:layout_marginLeft="15dp"
                android:textStyle="bold"
                android:textSize="17sp"
                android:text="0000"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="7.5dp"
                android:layout_gravity="center"
                android:src="@mipmap/iv_no_addcar"/>
            <View
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="22"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="8dp"
                android:text="自动支付"
                android:textColor="#333"
                android:textSize="14sp"
                android:layout_gravity="center"/>

            <ToggleButton
                android:id="@+id/tog_btn_IsPayAuto"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginRight="15dp"
                android:background="@android:color/transparent"
                android:button="@drawable/switch_btn_bg"
                android:text=""
                android:textOff=""
                android:textOn="" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#e5e5e5"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"/>
        <LinearLayout
            android:id="@+id/ll_go_renzheng"
            android:layout_width="match_parent"
            android:layout_height="39dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="马上认证"
                android:layout_gravity="center"
                android:layout_weight="22"
                android:layout_marginLeft="15dp"
                android:textSize="15sp"
                android:textColor="#333"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginRight="15dp"
                android:src="@mipmap/right_xh" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#e5e5e5"/>
    </LinearLayout>
    <View
        android:id="@+id/view_ing_renzheng"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:visibility="gone"/>
    <LinearLayout
        android:id="@+id/ll_ing_renzheng"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="#fff"
        android:visibility="gone">
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#e5e5e5"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="57dp"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/tv_listCarNum2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#333"
                android:layout_gravity="center"
                android:layout_marginLeft="15dp"
                android:textStyle="bold"
                android:textSize="17sp"
                android:text="0000"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="7.5dp"
                android:layout_gravity="center"
                android:src="@mipmap/iv_ing_addcar"/>
            <View
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="22"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="8dp"
                android:text="自动支付"
                android:textColor="#333"
                android:textSize="14sp"
                android:layout_gravity="center"/>

            <ToggleButton
                android:id="@+id/tog_btn_IsPayAuto2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginRight="15dp"
                android:background="@android:color/transparent"
                android:button="@drawable/switch_btn_bg"
                android:text=""
                android:textOff=""
                android:textOn="" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#e5e5e5"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="39dp"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/tv_addname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="审核时间需要1-2工作日，"
                android:layout_gravity="center"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:textSize="14sp"
                android:textColor="#999"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#e5e5e5"/>
    </LinearLayout>
    <View
        android:id="@+id/view_ok_renzheng"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:visibility="gone"/>
    <LinearLayout
        android:id="@+id/ll_ok_renzheng"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="#fff">
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#e5e5e5"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="57dp"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/tv_listCarNum3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#333"
                android:layout_gravity="center"
                android:layout_marginLeft="15dp"
                android:textStyle="bold"
                android:textSize="17sp"
                android:text="0000"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="7.5dp"
                android:layout_gravity="center"
                android:src="@mipmap/iv_ok_addcar"
                android:visibility="gone"/>
            <View
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="22"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="8dp"
                android:text="自动支付"
                android:textColor="#333"
                android:textSize="14sp"
                android:layout_gravity="center"/>

            <ToggleButton
                android:id="@+id/tog_btn_IsPayAuto3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginRight="15dp"
                android:background="@android:color/transparent"
                android:button="@drawable/switch_btn_bg"
                android:text=""
                android:textOff=""
                android:textOn="" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#e5e5e5"/>
    </LinearLayout>
</LinearLayout>
